{
    "display": {
        "showFunctionNames": true,
        "showDataTypes": true,
        "expandFunctionDetails": true,
        "showParameters": true,
        "showVariables": true,
        "maxContentLines": 50
    },
    "navigation": {
        "enableFileOpening": true,
        "defaultEditor": "",
        "autoExpandFirstLevel": false,
        "useVimKeys": true,
        "allowNavigationToggle": true
    },
    "colors": {
        "fileColor": "Green",
        "classColor": "Yellow",
        "functionColor": "Magenta",
        "parameterColor": "Cyan",
        "variableColor": "White",
        "selectedColor": "Cyan",
        "errorColor": "Red"
    },
    "parsing": {
        "includePrivateFunctions": true,
        "includeComments": false,
        "parseVariableAssignments": true,
        "parseParameterTypes": true
    }
}
